    Protected Sub LlenarControles()
        Try
            Dim bizPromocionesLN As New promocionLN
            Dim xmlPromociones As XmlDocument = bizPromocionesLN.ConsultarVigentes

            Dim dsPromo As New DataSet
            dsPromo.ReadXml(New XmlNodeReader(xmlPromociones))

            grvPromos.DataSource = dsPromo
            grvPromos.DataMember = "Promocion"
            grvPromos.DataBind()
        Catch ex As Exception
            cuGeneral.IsValid = False
            cuGeneral.ErrorMessage = ex.Message
        End Try
    End Sub

